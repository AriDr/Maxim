--I Would Recommend 4 Or More For The Teleport Time
getgenv().api = loadstring(game:HttpGet("https://raw.githubusercontent.com/AriDr/Maxim/main/Api"))()
local canTeleport = true
game:GetService("Workspace").Collectibles.ChildAdded:Connect(function(Tokens)
    if canTeleport == true and Enabled == true and Tokens:FindFirstChild("FrontDecal") then
        if api.rmagnitude(game:GetService("Workspace").FlowerZones["Stump Field"].Position - Tokens.Position) <= 5 then
            canTeleport = false
            api:pathfind(Tokens.Position)
            if api.rmagnitude(api.humanoidrootpart.Position - Tokens.Position) <= 1 then
            task.wait(0.5)
                canTeleport = true
            end
        end    
    end
end)